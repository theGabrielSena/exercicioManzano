package logicaProgramacaoFaccat;

import java.util.Scanner;

public class EX59 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int soma=0;
		int n1 = sc.nextInt();
		System.out.println("Digite o primeiro número:");
		int n2 = sc.nextInt();
		System.out.println("Digite o segundo número:");
		int n3 = sc.nextInt();
		System.out.println("Digite o terceiro número:");
		int n4 = sc.nextInt();
		System.out.println("Digite o quarto número:");
		int n5 = sc.nextInt();
		System.out.println("Digite o quinto número:");
		int n6 = sc.nextInt();
		System.out.println("Digite o sexto número:");
		int n7 = sc.nextInt();
		System.out.println("Digite o sétimo número:");
		int n8 = sc.nextInt();
		System.out.println("Digite o oitavo número:");
		int n9 = sc.nextInt();
		System.out.println("Digite o nono número:");
		int n10 = sc.nextInt();
		System.out.println("Digite o decimo número:");
		
		if(n1<0||n2<0) {
			soma++;
		}
//		if(n2<0) {
//			soma++;
//		}
		if(n3<0) {
			soma++;
		}
		if(n4<0) {
			soma++;
		}
		if(n5<0) {
			soma++;
		}
		if(n6<0) {
			soma++;
		}
		if(n7<0) {
			soma++;
		}
		if(n8<0) {
			soma++;
		}
		if(n9<0) {
			soma++;
		}
		if(n10<0) {
			soma++;
		}
		System.out.println(soma+" dos numeros são negativos");
	}

}
