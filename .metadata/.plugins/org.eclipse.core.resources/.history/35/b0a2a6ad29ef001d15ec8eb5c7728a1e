package logicaProgramacaoFaccat;

import java.util.Scanner;

public class EX54 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		
		int n;
		do{
			System.out.println("Digite um número");
			n = sc.nextInt();
		}while(n<0);
		
		for (int i = 1; i <= n; i++) {
			System.out.println(i);
		}
		sc.close();
		
	}

}
