package logicaProgramacaoFaccat;

import java.util.Scanner;

public class EX53 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Digite um número");
		int n = sc.nextInt();
		
		for (int i = 1; i <= n; i++) {
			System.out.println(i);
		}
		sc.close();
	}

}
